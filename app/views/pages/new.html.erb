<div id='outlet' data-component='Pages' data-view='New'>
	<h1>Create new page</h1>
  <%= form_for @new_page do |f| %>
    <div>
      <p><%= f.text_field :name, :placeholder => :name %></p>
    </div>
    <%= f.submit :save, class: "btn btn-primary" %>
  <% end %>
</div>

<%= simple_form_for @container, url: containers_path do |form| %>
  <%= form.input :image, as: :file %>
<% end %>
